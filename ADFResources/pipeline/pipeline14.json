{
	"name": "pipeline14",
	"properties": {
		"activities": [
			{
				"name": "Script1",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 31,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "Oracle2",
					"type": "LinkedServiceReference"
				},
				"typeProperties": {
					"scripts": [
						{
							"type": "Query",
							"text": {
								"value": "SELECT NAME1 FROM SAPSR3.KNA1 WHERE MANDT = @{pipeline().parameters.MANDT} AND KUNNR = (SELECT KUNNR FROM SAPSR3.VBPA WHERE MANDT = @{pipeline().parameters.MANDT} AND VBELN =@{pipeline().parameters.VBELN} AND PARVW = @{pipeline().parameters.PARVW})",
								"type": "Expression"
							}
						}
					],
					"scriptBlockExecutionTimeout": "02:00:00"
				}
			}
		],
		"parameters": {
			"MANDT": {
				"type": "string",
				"defaultValue": "'110'"
			},
			"VBELN": {
				"type": "string",
				"defaultValue": "'1020000125'"
			},
			"PARVW": {
				"type": "string",
				"defaultValue": "'WE'"
			}
		},
		"annotations": []
	}
}