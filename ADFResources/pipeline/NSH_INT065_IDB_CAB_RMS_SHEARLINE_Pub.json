						"type": "OracleSource",
						"oracleReaderQuery": {
							"value": "WITH FirstQuery AS (\n    SELECT r_run_no, r_dia_ref, l_comb_no, d_section_no, d_bin_number, d_cutting_seq_no\n    FROM (\n        SELECT r_run_no, r_dia_ref, l_comb_no, d_section_no, d_bin_number, d_cutting_seq_no\n        FROM scm_admin.cab_rms_shearline_download\n        MINUS\n        SELECT r_run_no, r_dia_ref, l_comb_no, d_section_no, d_bin_number, d_cutting_seq_no\n        FROM scm_admin.esb_cab_rms_shearline_download\n    )\n    WHERE ROWNUM <= '${batchSize}'\n)\nSELECT \n    NVL(FQ.R_RUN_NO , ' ') AS R_RUN_NO,\n    NVL(FQ.R_PROD_TYPE_REF , ' ') AS R_PROD_TYPE_REF,\n    NVL(FQ.R_START_STOCK_LEN , 0) AS R_START_STOCK_LEN,\n    NVL(FQ.R_DIA_REF , 0) AS R_DIA_REF,\n    NVL(FQ.L_COMB_NO, ' ') AS L_COMB_NO,\n    NVL(FQ.L_QTY_OF_SEGMENT , 0) AS L_QTY_OF_SEGMENT,\n    NVL(FQ.L_STOCK_BARS_LEN , 0) AS L_STOCK_BARS_LEN,\n    NVL(FQ.D_NO_OF_CUTS , 0) AS D_NO_OF_CUTS,\n    NVL(FQ.D_SECTION_NO , ' ') AS D_SECTION_NO,\n    NVL(FQ.D_BIN_NUMBER , ' ') AS D_BIN_NUMBER,\n    NVL(FQ.D_CUT_LENGTH, ' ') AS D_CUT_LENGTH,\n    NVL(FQ.D_TAG_REF_INFO, ' ') AS D_TAG_REF_INFO,\n    NVL(FQ.D_STRAIGHT_BEND_FLAG, ' ') AS D_STRAIGHT_BEND_FLAG,\n    NVL(FQ.D_BBS_BAR_MARK_INFO, ' ') AS D_BBS_BAR_MARK_INFO,\n    NVL(FQ.D_TAG_NO, ' ') AS D_TAG_NO,\n    NVL(FQ.D_FLAG_INFO, ' ') AS D_FLAG_INFO,\n    NVL(FQ.CREATE_DATE , ' ') AS CREATE_DATE,\n    NVL(FQ.UPDATE_DATE , ' ') AS UPDATE_DATE,\n    NVL(FQ.D_CUTTING_SEQ_NO, 0) AS D_CUTTING_SEQ_NO,\n    NVL(FQ.D_OFFCUT_IND, ' ') AS D_OFFCUT_IND\nFROM  \n    FirstQuery FQ\nWHERE \n    FQ.R_RUN_NO IN ('${r_run_no}') \n    AND FQ.R_DIA_REF IN ('${r_dia_ref}') \n    AND FQ.L_COMB_NO IN ('${l_comb_no}') \n    AND FQ.D_SECTION_NO IN ('${d_section_no}') \n    AND FQ.D_BIN_NUMBER IN ('${d_bin_number}') \n    AND FQ.D_CUTTING_SEQ_NO IN ('${d_cutting_seq_no}')\n",
							"type": "Expression"
						},
