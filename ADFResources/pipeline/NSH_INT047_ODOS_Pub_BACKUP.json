esh_cw_wt_kg,\r\n    mesh_cw_bvbs,\r\n    mesh_no_of_bend,\r\n    mesh_no_of_cut,\r\n    mesh_manual_trim_ind,\r\n    mesh_bom_ind,\r\n    meshb_wire_line_cross,\r\n    meshb_wire_seq_no,\r\n    TRIM(meshb_wire_mat_code) AS meshb_wire_mat_code,\r\n    meshb_wire_length,\r\n    meshb_wire_dia,\r\n    COALESCE(meshb_wire_wt_kg, 0.0) AS meshb_wire_wt_kg,\r\n    COALESCE(CAST(meshb_wire_qty AS INT), 0) AS meshb_wire_qty,\r\n    COALESCE(CAST(meshb_wire_spacing AS INT), 0) AS meshb_wire_spacing,\r\n    bpc_cage_dia,\r\n    bpc_cage_length,\r\n    TRIM(bpc_cage_set_code) AS bpc_cage_set_code,\r\n    bpc_centralizer_qty,\r\n    bpc_lap_length,\r\n    bpc_end_length,\r\n    bpc_pile_dia,\r\n    bpc_l2_cage_dia,\r\n    bpc_no_rings_start,\r\n    TRIM(bpc_sleeve_id) as bpc_sleeve_id,\r\n    TRIM(bpc_jig_id) as bpc_jig_id,\r\n    TRIM(bpc_add_jig_id) as bpc_add_jig_id,\r\n    bpc_pile_seq_no,\r\n    bpc_micropile_ind,\r\n    bpc_no_rings_end,\r\n    COALESCE(TRIM(pcg_cage_type), ' ') AS pcg_cage_type,\r\n    COALESCE(TRIM(pcg_assembly_ind), ' ') AS pcg_assembly_ind,\r\n    TRIM(remarks) AS remarks,\r\n    time_stamp,\r\n    TRIM(st_element) AS st_element,\r\n    process_status,\r\n    transfer_idref,\r\n    transfer_status,\r\n    create_by,\r\n    create_date,\r\n    update_by,\r\n    update_date,\r\n    qty_in_base_uom,\r\n    base_uom,\r\n    QA_transfer_status,\r\n    COALESCE(drawing_no_rev, ' ') AS drawing_no_rev,\r\n    COALESCE(drawing_no, ' ') AS drawing_no,\r\n    COALESCE(inv_len_in_mm, 0.0) AS inv_len_in_mm,\r\n    COALESCE(grade_dia, ' ') AS grade_dia,\r\n    COALESCE(bpc_layer, 0) AS bpc_layer,\r\n    COALESCE(cab_main_bar_angle, ' ') AS cab_main_bar_angle,\r\n    COALESCE(sap_product_group, ' ') AS sap_product_group,\r\n    COALESCE(product_marking_rev_no, ' ') AS product_marking_rev_no,\r\n    COALESCE(cab_standard_end_no, 0) AS cab_standard_end_no,\r\n    COALESCE(cab_former_type, ' ') AS cab_former_type,\r\n    bpc_pile_no,\r\n    order_no,\r\n    product_type,\r\n    cab_extended_end_no,\r\n    cab_stud_end_no,\r\n    prc_bpc_layer_no,\r\n    bbs_page_no,\r\n    mesh_product_code,\r\n    mesh_lw_inv_len,\r\n    mesh_cw_inv_len,\r\n    mesh_inv_mo1,\r\n    mesh_inv_mo2,\r\n    mesh_inv_co1,\r\n    mesh_inv_co2,\r\n    mesh_slope_indicator,\r\n    mesh_num_of_links,\r\n    meshb_MESH_TYPE,\r\n    meshb_START_POS,\r\n    meshb_WIRE_SPEC,\r\n    meshb_NO_PITCH,\r\n    meshb_REP_FROM,\r\n    meshb_REP_TO,\r\n    meshb_num_of_REPETITION,\r\n    meshb_WIRE_PROD_CODE,\r\n    meshb_TWIN_IND,\r\n    meshb_MACH_WIRE_SPEC,\r\n    mesh_slope,\r\n    prc_panel_no,\r\n    prc_panel_seq_no,\r\n    IDENTICAL_HASHCODE,\r\n    SIMILAR_MAINBAR_HASHCODE,\r\n    SIMILAR_EXTLINK_HASHCODE,\r\n    COALESCE(COMPLEXITY_IND, 0.0) AS COMPLEXITY_IND\r\nFROM dbo.SCM_Order_Item\r\nWHERE order_request_no IN (SELECT ORDER_REQUEST_NO FROM SCM_Order_Header);\r\n",
