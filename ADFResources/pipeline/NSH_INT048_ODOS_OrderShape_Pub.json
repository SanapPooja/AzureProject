				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT \r\n    t1.IDENTITY_NO,\r\n    t1.ORD_REQ_NO,\r\n    t1.SALES_ORDER,\r\n    t1.ITEM_NO,\r\n    t1.SHAPE_IND,\r\n    t1.SHAPE_CD,\r\n    t1.DIM_SEQ,\r\n    t1.DIM_CHAR,\r\n    t1.DIM_TYPE,\r\n    t1.DIM_VALUE,\r\n    t1.DIM_X,\r\n    t1.DIM_Y,\r\n    t1.DIM_Z,\r\n    t1.CRITICAL_IND,\r\n    t1.BEND_SEQ,\r\n    t1.PRINT_TAG,\r\n    t1.STATUS,\r\n    t1.PROC_TIMESTAMP,\r\n    t1.WBSElementId,\r\n    t2.PROD_GRP\r\nFROM \r\n    SAP_ORD_SHAPE_VARIANT_DSS t1\r\nJOIN \r\n    SAP_ORDER_ITEM t2 ON t1.ORD_REQ_NO = t2.ORD_REQ_NO AND t1.ITEM_NO = t2.ITEM_NO\r\nWHERE \r\n    t1.ORD_REQ_NO IN (\r\n        SELECT sod.order_request_no\r\n        FROM dbo.SCM_Order_Header sod\r\n        JOIN WMBORDERSHAPEbj1ugcj wmb ON sod.order_request_no = wmb.order_request_no\r\n    )\r\n    AND t1.ITEM_NO IN (\r\n        SELECT DISTINCT ITEM_NO \r\n        FROM SAP_ORD_SHAPE_VARIANT_DSS \r\n        WHERE ORD_REQ_NO IN (\r\n            SELECT sod.order_request_no\r\n            FROM dbo.SCM_Order_Header sod\r\n            JOIN WMBORDERSHAPEbj1ugcj wmb ON sod.order_request_no = wmb.order_request_no\r\n        )\r\n    );\r\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
