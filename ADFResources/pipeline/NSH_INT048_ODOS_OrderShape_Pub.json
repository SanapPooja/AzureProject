							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": "DECLARE @cols AS NVARCHAR(MAX),\r\n        @query AS NVARCHAR(MAX),\r\n        @topN INT = 20, -- Number of dimension sets to pivot\r\n        @maxCounter INT = 20; -- Maximum counter value for the loop\r\n\r\n-- Initialize the column strings\r\nSET @cols = '';\r\n\r\n-- Generate column aliases\r\nSELECT @cols = @cols +\r\n    'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN DIM_X END) AS DIM_X' + CAST(number AS VARCHAR(10)) + ', ' +\r\n    'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN DIM_Y END) AS DIM_Y' + CAST(number AS VARCHAR(10)) + ', ' +\r\n    'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN DIM_CHAR END) AS DIM_CHAR' + CAST(number AS VARCHAR(10)) + ', ' +\r\n    'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN DIM_TYPE END) AS DIM_TYPE' + CAST(number AS VARCHAR(10)) + ', ' +\r\n    'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN DIM_VALUE END) AS DIM_VALUE' + CAST(number AS VARCHAR(10)) + ', ' +\r\n    (CASE WHEN number = @maxCounter THEN 'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN CRITICAL_IND END) AS CRITICAL_IND20'\r\n     ELSE 'MAX(CASE WHEN GroupedRowNum = ' + CAST(number AS VARCHAR(10)) + ' THEN CRITICAL_IND END) AS CRITICAL_IND' + CAST(number AS VARCHAR(10)) END) + ', '\r\nFROM master..spt_values WHERE type = 'P' AND number BETWEEN 1 AND @maxCounter;\r\n\r\n-- Remove trailing comma and space\r\nSET @cols = LEFT(@cols, LEN(@cols) - 1);\r\n\r\n-- Generate the full dynamic SQL query\r\nSET @query = '\r\nWITH RankedData AS (\r\n    SELECT \r\n        t1.ORD_REQ_NO,\r\n        t1.ITEM_NO,\r\n        t1.DIM_X,\r\n        t1.DIM_Y,\r\n        t1.DIM_CHAR,\r\n        t1.DIM_TYPE,\r\n        t1.DIM_VALUE,\r\n        t1.CRITICAL_IND,\r\n\t\tt2.PROD_GRP,\r\n\t\tCASE \r\n            WHEN t3.PRODTYPE_ID = ''7'' THEN ''MSH''\r\n            WHEN t3.PRODTYPE_ID = ''4'' THEN ''CAB''\r\n            WHEN t3.PRODTYPE_ID = ''9'' THEN ''BPC''\r\n            WHEN t3.PRODTYPE_ID = ''10'' THEN ''PRC''\r\n            WHEN t3.PRODTYPE_ID = ''14'' THEN ''PRC''\r\n        END AS PRODUCT_TYPE,\r\n        ROW_NUMBER() OVER (PARTITION BY t1.ORD_REQ_NO, t1.ITEM_NO ORDER BY t1.ITEM_NO) AS GroupedRowNum\r\n    FROM\r\n        SAP_ORD_SHAPE_VARIANT_DSS t1\r\n\t\tINNER JOIN \r\n    dbo.SAP_ORDER_ITEM t2 \r\n    ON t1.ORD_REQ_NO = t2.ORD_REQ_NO \r\n    AND t1.ITEM_NO = t2.ITEM_NO\r\n\tINNER JOIN \r\n    dbo.SAP_WBS_SOR t3 \r\n    ON t1.ORD_REQ_NO = t3.ORD_REQ_NO\r\n\tWHERE  \r\n    t1.ORD_REQ_NO IN (SELECT \r\n\t\t\t\t\t\t\tsod.order_request_no\r\n\t\t\t\t\t\tFROM dbo.SCM_Order_Header sod\r\n\t\t\t\t\t\tJOIN \r\n\t\t\t\t\t\t\t WMBORDERSHAPEbj1ugcj wmb\r\n\t\t\t\t\t\tON \r\n\t\t\t\t\t\t\t sod.order_request_no = wmb.order_request_no WHERE sod.transfer_status=''P'')\r\n    AND t1.ITEM_NO IN (select distinct ITEM_NO from SAP_ORD_SHAPE_VARIANT_DSS where ORD_REQ_NO IN (SELECT \r\n\t\t\t\t\t\t\t sod.order_request_no\r\n\t\t\t\t\t\tFROM dbo.SCM_Order_Header sod\r\n\t\t\t\t\t\tJOIN \r\n\t\t\t\t\t\t\t WMBORDERSHAPEbj1ugcj wmb\r\n\t\t\t\t\t\tON \r\n\t\t\t\t\t\t\t sod.order_request_no = wmb.order_request_no WHERE sod.transfer_status=''P''))\r\n    AND (\r\n        (t3.PRODTYPE_ID = ''7'') \r\n        OR \r\n        (t3.PRODTYPE_ID != ''7'' AND (t1.PRINT_TAG = ''Y'' OR t1.PRINT_TAG IS null))\r\n    )\r\n)\r\nSELECT\r\n    ORD_REQ_NO,\r\n    ITEM_NO,\r\n\tPROD_GRP,\r\n\tPRODUCT_TYPE,\r\n    ' + @cols + '\r\nFROM RankedData\r\nGROUP BY ORD_REQ_NO, ITEM_NO,PROD_GRP,PRODUCT_TYPE\r\nORDER BY ORD_REQ_NO, ITEM_NO;';\r\n\r\n-- Execute the dynamic SQL\r\nEXEC sp_executesql @query;",
									"queryTimeout": "02:00:00",
