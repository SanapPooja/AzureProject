								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": "DECLARE @cols AS NVARCHAR(MAX),\n        @query AS NVARCHAR(MAX),\n        @topN INT = 20, -- Number of dimension sets to pivot\n        @counter INT = 1, -- Counter for the loop\n        @maxCounter INT = 20; -- Maximum counter value for the loop\n \n-- Initialize the column strings\nSET @cols = '';\n \n-- Loop to generate column aliases\nWHILE @counter <= @maxCounter\nBEGIN\n    SET @cols = @cols + \n        'MAX(CASE WHEN GroupedRowNum = ' + CAST(@counter AS VARCHAR(10)) + ' THEN DIM_X END) AS DIM_X' + CAST(@counter AS VARCHAR(10)) + ', ' +\n        'MAX(CASE WHEN GroupedRowNum = ' + CAST(@counter AS VARCHAR(10)) + ' THEN DIM_Y END) AS DIM_Y' + CAST(@counter AS VARCHAR(10)) + ', ' +\n        'MAX(CASE WHEN GroupedRowNum = ' + CAST(@counter AS VARCHAR(10)) + ' THEN DIM_CHAR END) AS DIM_CHAR' + CAST(@counter AS VARCHAR(10)) + ', ' +\n        'MAX(CASE WHEN GroupedRowNum = ' + CAST(@counter AS VARCHAR(10)) + ' THEN DIM_TYPE END) AS DIM_TYPE' + CAST(@counter AS VARCHAR(10)) + ', ' +\n        'MAX(CASE WHEN GroupedRowNum = ' + CAST(@counter AS VARCHAR(10)) + ' THEN DIM_VALUE END) AS DIM_VALUE' + CAST(@counter AS VARCHAR(10)) + ', ' +\n        'MAX(CASE WHEN GroupedRowNum = ' + CAST(@counter AS VARCHAR(10)) + ' THEN CRITICAL_IND END) AS CRITICAL_IND' + CAST(@counter AS VARCHAR(10)) + ', ';\n    SET @counter = @counter + 1;\nEND;\n \n-- Remove trailing comma and space\nSET @cols = LEFT(@cols, LEN(@cols) - 2);\n \n-- Generate the full dynamic SQL query\nSET @query = '\nWITH RankedData AS (\n    SELECT top 100\n        ORD_REQ_NO,\n        ITEM_NO,\n        DIM_X,\n        DIM_Y,\n        DIM_CHAR,\n        DIM_TYPE,\n        DIM_VALUE,\n        CRITICAL_IND,\n        ROW_NUMBER() OVER (PARTITION BY ORD_REQ_NO ORDER BY ITEM_NO) AS RowNum,\n        CEILING(CAST(ROW_NUMBER() OVER (PARTITION BY ORD_REQ_NO ORDER BY ITEM_NO) AS FLOAT) / ' + CAST(@topN AS NVARCHAR) + ') AS GroupNum,\n        ROW_NUMBER() OVER (PARTITION BY ORD_REQ_NO ORDER BY ITEM_NO) AS GroupedRowNum\n    FROM \n        SAP_ORD_SHAPE_VARIANT_DSS \n)\nSELECT \n    ORD_REQ_NO,\n    ITEM_NO,\n    ' + @cols + '\nFROM RankedData\nGROUP BY ORD_REQ_NO, ITEM_NO, GroupNum, GroupedRowNum\nORDER BY ORD_REQ_NO, GroupNum, GroupedRowNum;';",
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
