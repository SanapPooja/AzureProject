					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "SELECT \r\n    (COUNT(1)/ 50) + 1 AS recordcount\r\nFROM \r\n    dbo.SCM_Order_Header\r\nWHERE \r\n    order_request_no IN (\r\n        SELECT \r\n            t1.order_request_no\r\n        FROM \r\n            dbo.SCM_Order_Header t1\r\n        JOIN \r\n            WMBORDERSHAPEbj1ugcj t2 ON t1.order_request_no = t2.order_request_no\r\n    );\r\n",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
