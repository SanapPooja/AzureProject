					"source": {
						"type": "OracleSource",
						"oracleReaderQuery": {
							"value": "WITH FirstQuery AS (\n    SELECT \n        FLD_PSCH_Q_PO_NO,\n        FLD_PSCH_Q_SEQ_NO,\n        FLD_PSCH_Q_PRODN_ORD_NO,\n        FLD_PSCH_TAG_ID_FROM,\n        FLD_PSCH_PLAN_WRK_CTR\n    FROM (\n        SELECT \n            FLD_PSCH_Q_PO_NO,\n            FLD_PSCH_Q_SEQ_NO,\n            FLD_PSCH_Q_PRODN_ORD_NO,\n            FLD_PSCH_TAG_ID_FROM,\n            FLD_PSCH_PLAN_WRK_CTR\n        FROM scm_admin.cabsequencing\n        MINUS\n        SELECT \n            FLD_PSCH_Q_PO_NO,\n            FLD_PSCH_Q_SEQ_NO,\n            FLD_PSCH_Q_PRODN_ORD_NO,\n            FLD_PSCH_TAG_ID_FROM,\n            FLD_PSCH_PLAN_WRK_CTR\n        FROM scm_admin.esb_cabsequencing\n    )\n    WHERE rownum <= '1000'\n)\nSELECT\t\t\t\t \t\t\t\t\n    NVL(FSQ.FLD_PSCH_Q_PO_NO, ' ') AS FLD_PSCH_Q_PO_NO,\n    NVL(FSQ.FLD_PSCH_Q_SEQ_NO, 0) AS FLD_PSCH_Q_SEQ_NO,\n    NVL(FLD_PSCH_Q_OPER_TYPE, ' ') AS FLD_PSCH_Q_OPER_TYPE,\n    NVL(FLD_PSCH_Q_START_TIME, ' ') AS FLD_PSCH_Q_START_TIME,\n    NVL(FLD_PSCH_Q_END_TIME, ' ') AS FLD_PSCH_Q_END_TIME,\n    NVL(FLD_PSCH_PLAN_PROCESS_TIME, 0) AS FLD_PSCH_PLAN_PROCESS_TIME,\n    NVL(FLD_PSCH_MSG_ID, ' ') AS FLD_PSCH_MSG_ID,\n    NVL(FLD_PSCH_CUST_ID, ' ') AS FLD_PSCH_CUST_ID,\n    SUBSTR(REPLACE(FLD_PSCH_CUST_DESC,'''','\"'),0,50) AS FLD_PSCH_CUST_DESC,\n    SUBSTR(REPLACE(FLD_PSCH_SHIP_TO_DESC,'''','\"'),0,50) AS FLD_PSCH_SHIP_TO_DESC,\n    NVL(FSQ.FLD_PSCH_Q_PRODN_ORD_NO, ' ') AS FLD_PSCH_Q_PRODN_ORD_NO,\n    NVL(FLD_PSCH_PRODN_ORD_NO, ' ') AS FLD_PSCH_PRODN_ORD_NO,\n    NVL(FLD_PSCH_SAL_ORD_NO, ' ') AS FLD_PSCH_SAL_ORD_NO,\n    NVL(FLD_PSCH_ITEM_NO, ' ') AS FLD_PSCH_ITEM_NO,\n    NVL(FLD_PSCH_MATL_NO, ' ') AS FLD_PSCH_MATL_NO,\n    NVL(FLD_PSCH_PROD_GROUP, ' ') AS FLD_PSCH_PROD_GROUP,\n    NVL(FLD_PSCH_SHAPE_CODE_ID, ' ') AS FLD_PSCH_SHAPE_CODE_ID,\n    NVL(FLD_PSCH_SAL_FG_QTY, 0) AS FLD_PSCH_SAL_FG_QTY,\n    NVL(FLD_PSCH_SAL_FG_UOM, ' ') AS FLD_PSCH_SAL_FG_UOM,\n    NVL(FLD_PSCH_PRODN_QTY_PCS, 0) AS FLD_PSCH_PRODN_QTY_PCS,\n    NVL(FLD_PSCH_PLAN_PRODN_DATE, ' ') AS FLD_PSCH_PLAN_PRODN_DATE,\n    NVL(FLD_PSCH_PLAN_PRODN_SHIFT, ' ') AS FLD_PSCH_PLAN_PRODN_SHIFT,\n    SUBSTR(REPLACE(FLD_PSCH_RUN_NO,'''','\"'),0,12) AS FLD_PSCH_RUN_NO,\n    NVL(FSQ.FLD_PSCH_TAG_ID_FROM, ' ') AS FLD_PSCH_TAG_ID_FROM,\n    NVL(FLD_PSCH_DEPNT_REQ_FLAG, ' ') AS FLD_PSCH_DEPNT_REQ_FLAG,\n    NVL(FLD_PSCH_INDEPNT_ITEM, 0) AS FLD_PSCH_INDEPNT_ITEM,\n    NVL(FLD_PSCH_INDEPNT_MATL_NO, ' ') AS FLD_PSCH_INDEPNT_MATL_NO,\n    NVL(FLD_PSCH_ENG_DWG_NO, ' ') AS FLD_PSCH_ENG_DWG_NO,\n    NVL(FSQ.FLD_PSCH_PLAN_WRK_CTR, ' ') AS FLD_PSCH_PLAN_WRK_CTR,\n    NVL(FLD_PSCH_PLAN_SETUP_TIME, 0) AS FLD_PSCH_PLAN_SETUP_TIME,\n    NVL(FLD_PSCH_SEQ_NO, '0') AS FLD_PSCH_SEQ_NO,\n    NVL(FLD_PSCH_NXT_WRK_CTR, ' ') AS FLD_PSCH_NXT_WRK_CTR,\n    NVL(FLD_PSCH_Q_ISRELEASE, ' ') AS FLD_PSCH_Q_ISRELEASE,\n    NVL(FLD_PSCH_Q_RELEASE_DATE, ' ') AS FLD_PSCH_Q_RELEASE_DATE,\n    NVL(FLD_PSCH_Q_CREATE_DATE, ' ') AS FLD_PSCH_Q_CREATE_DATE,\n    NVL(FLD_PSCH_Q_MODIFY_DATE, ' ') AS FLD_PSCH_Q_MODIFY_DATE,\n    NVL(FLD_PSCH_RELEASE_CODE, ' ') AS FLD_PSCH_RELEASE_CODE,\n    NVL(FLD_PSCH_PU_NR_OF_OPERATION, 0) AS FLD_PSCH_PU_NR_OF_OPERATION,\n    CASE \n        WHEN FLD_PSCH_UOPQUANTITY < 0 THEN '1'\n        ELSE NVL(TO_CHAR(FLD_PSCH_UOPQUANTITY), '0')\n    END AS FLD_PSCH_UOPQUANTITY,\n    NVL(FLD_PSCH_UOPQUANTITYUOM, ' ') AS FLD_PSCH_UOPQUANTITYUOM\nFROM scm_admin.cabsequencing FSQ \nJOIN FirstQuery FQ ON \n    FSQ.FLD_PSCH_Q_PO_NO = FQ.FLD_PSCH_Q_PO_NO AND\n    FSQ.FLD_PSCH_Q_SEQ_NO = FQ.FLD_PSCH_Q_SEQ_NO AND\n    FSQ.FLD_PSCH_Q_PRODN_ORD_NO = FQ.FLD_PSCH_Q_PRODN_ORD_NO AND\n    FSQ.FLD_PSCH_TAG_ID_FROM = FQ.FLD_PSCH_TAG_ID_FROM AND\n    FSQ.FLD_PSCH_PLAN_WRK_CTR = FQ.FLD_PSCH_PLAN_WRK_CTR;\n",
							"type": "Expression"
