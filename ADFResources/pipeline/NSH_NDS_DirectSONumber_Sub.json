{
	"name": "NSH_NDS_DirectSONumber_Sub",
	"properties": {
		"activities": [
			{
				"name": "PostQueueToODOS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "POST",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['$ORDER_REQUEST_NO']"
								},
								"sink": {
									"name": "SOR_Number",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['$SALES_ORDER']"
								},
								"sink": {
									"name": "SO_Number",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['$TM_ACCESS']"
								},
								"sink": {
									"name": "TM_Access"
								}
							},
							{
								"source": {
									"path": "$['$Inserted_Date']"
								},
								"sink": {
									"name": "Inserted_date"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DatasetQueueHeadDemo",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SAP_SO_SOR_DEMO",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Wait_For_60sec",
				"type": "Wait",
				"dependsOn": [
					{
						"activity": "PostQueueToODOS",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"waitTimeInSeconds": 60
				}
			},
			{
				"name": "DeleteFromQueue",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "Wait_For_60sec",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "DELETE",
					"url": "https://sb-nsh-esb-dev.servicebus.windows.net/sap_so_number/messages/head",
					"authentication": {
						"type": "MSI",
						"resource": "https://servicebus.azure.net/"
					}
				}
			}
		],
		"annotations": []
	}
}